{
	"extends": "@sapphire/ts-config/extra-strict-without-decorators",
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"#prompts/*": ["src/prompts/*.ts"],
			"#lib/*": ["src/lib/*.ts"],
			"#functions/*": ["src/functions/*.ts"],
			"#commands/*": ["src/commands/*.ts"],
			"#constants": ["src/constants.ts"]
		},
		"moduleResolution": "node16"
	}
}
